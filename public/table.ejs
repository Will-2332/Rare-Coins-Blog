<!DOCTYPE html>
<html>
<head>
  <meta charset='utf-8'>
  <meta http-equiv='X-UA-Compatible' content='IE=edge'>
  <title>Page Title</title>
  <meta name='viewport' content='width=device-width, initial-scale=1'>
</head>
<body>
  
</body>
</html>
<table class="table table-striped table-primary">
  <thead>
    <tr>
      <th>Year/th>
      <th>Denomination</th>
      <th>Pic</th>
      <th>History</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <%if (coins.length>0){ %>
      <% coins.forEach(function(row){ %>
        <tr>
          <td>
            <%= row.Year %>
          </td>
          <td>
            <%= row.Denomination %>
          </td>
          <td>
            <%= row.Pic%>
          </td>
          <td>
            <%= row.History %>
          </td>
          <td>
            <%= row.Value %>
          </td>
        </tr>
        <% })}else{ %>
          <tr>
            <td colspan="6">No Record Found</td>

          </tr>
          <% } %>
  </tbody>
</table>